{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm install",
  "framework": "nuxtjs",
  "outputDirectory": ".output/public",
  "builds": [
    {
      "src": "nuxt.config.ts",
      "use": "@nuxtjs/vercel-builder"
    }
  ],
  "env": {
    "NITRO_PRESET": "vercel",
    "NODE_ENV": "production",
    "NUXT_PUBLIC_API_BASE": "/api"
  }
} 