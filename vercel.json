{
  "buildCommand": "prisma generate && nuxt build",
  "installCommand": "npm ci",
  "env": {
    "PRISMA_CLI_BINARY_TARGETS": "linux-musl,native",
    "NODE_ENV": "production",
    "SKIP_ENV_VALIDATION": "1"
  },
  "regions": ["iad1"],
  "crons": [],
  "framework": "nuxtjs",
  "functions": {
    "**/*.ts": {
      "runtime": "@vercel/node@3.0.7"
    },
    "**/*.js": {
      "runtime": "@vercel/node@3.0.7"
    }
  },
  "build": {
    "env": {
      "PRISMA_GENERATE_SKIP_AUTOINSTALL": "true"
    }
  }
} 