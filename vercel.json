{
  "buildCommand": "npm run build",
  "installCommand": "npm install && npx prisma generate",
  "env": {
    "PRISMA_CLI_BINARY_TARGETS": "rhel-openssl-1.0.x,native,linux-musl",
    "PRISMA_QUERY_ENGINE_LIBRARY": "rhel-openssl-1.0.x",
    "NODE_ENV": "production"
  },
  "regions": ["iad1"],
  "functions": {
    "server/**": {
      "runtime": "nodejs18.x"
    }
  }
} 